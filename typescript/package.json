{
  "name": "@geoprotocol/grc-20",
  "version": "0.1.0",
  "description": "GRC-20 TypeScript SDK for binary property graph encoding/decoding",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "grc-20",
    "graph",
    "knowledge-graph",
    "binary-format",
    "encoding"
  ],
  "author": "Geo Browser",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
